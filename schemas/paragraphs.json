          },
          "ParagraphID": {
            "type": "number"
          },
          "ParagraphNum": {
            "type": "number"
          },
          "CharID": {
            "type": "string"
          },
          "PlainText": {
            "type": "string"
          },
          "PhoneticText": {
            "type": "string"
          },
          "StemText": {
            "type": "string"
          },
          "ParagraphType": {
            "type": "string"
          },
          "Section": {
            "type": "number"
          },
          "Chapter": {
            "type": "number"
          },
          "CharCount": {
            "type": "number"
          },
          "WordCount": {
            "type": "number"
          },
          "Work": {
            "type": "object",
